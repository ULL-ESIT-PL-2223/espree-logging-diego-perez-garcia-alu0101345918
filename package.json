{
    "name": "@arcadiad/espree-logging-solution",
    "author": "Casiano Rodriguez Leon <crguezl@ull.edu.es> (https://crguezl.github.io/)",
    "description": "Adds logs to javascript code",
    "type": "module",
    "bin": {
        "funlog": "bin/log.js"
    },
    "scripts": {
        "exec": "node bin/log.js",
        "exec-out": "node bin/log.js -o ./bin/output.js",
        "test": "mocha test/test.mjs",
        "cov": "c8 npm test",
        "cov-docs": "c8 npm test -- --reporter=html --reporter=text --reports-dir docs",
        "jsdoc2md": "jsdoc2md --files src/*.js > jsdoc/README.md"
    },
    "dependencies": {
        "commander": "^10.0.0",
        "escodegen": "^2.0.0",
        "espree": "^9.4.1",
        "estraverse": "^5.2.0",
        "underscore": "^1.12.0"
    },
    "version": "1.1.1",
    "devDependencies": {
        "c8": "^7.13.0",
        "jsdoc-to-markdown": "^8.0.0",
        "mocha": "^10.2.0"
    }
}
